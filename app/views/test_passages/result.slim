h1 = t('.header', title: @test_passage.test.title)

b= "Your success rate is: #{@test_passage.correct_per_cent} %"

- if @test_passage.successful?
  p.test-success
    = 'Congratulations! You successfully passed the test.'
- else
  p.test-failure
    = 'You failed the test. Please, try again'

= link_to 'Back', tests_path
